
public class EscapeSequences {
	public static void main(String[] args){
		
		System.out.println("  ***\n");
		System.out.println(" *   *\n");
		System.out.println("*     *\n");
		System.out.println("*     *\n");
		System.out.println(" *   *\n");
		System.out.println("  ***\n");
		//System.out.println("\t\n\r*");
			}

}
 

 

